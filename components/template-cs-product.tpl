:root {
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "alignment",
    "editor": "listPicker",
    "list": {{ base_alignment_set }},
    "scope": "global"
  */
  --product-title-alignment: left;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "line_height",
    "editor": "rangePicker",
    "min": 0,
    "max": 5,
    "step": 0.1,
    "unit": "",
    "scope": "global"
  */
  --product-title-line-height: 1;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "size",
    "editor": "rangePicker",
    "min": 8,
    "max": 56,
    "unit": "px",
    "scope": "global"
  */
  --product-title-size: 24px;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "font_size",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "700",
      "off": "400"
    },
    "icon": "bold",
    "scope": "global"
  */
  --product-title-font-weight: 400;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "font_style",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "italic",
      "off": "normal"
    },
    "icon": "italic",
    "scope": "global"
  */
  --product-title-font-style: normal;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "text_decoration",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "underline",
      "off": "none"
    },
    "icon": "underline",
    "scope": "global"
  */
  --product-title-text-decoration: none;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "text_transform",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "uppercase",
      "off": "none"
    },
    "icon": "uppercase",
    "scope": "global"
  */
  --product-title-text-transform: none;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "color",
    "type": "button",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global"
  */
  --product-title-color: #000000;
  /* VoogStyle
    "pathI18n": ["product", "title"],
    "titleI18n": "hyphens",
    "editor": "listPicker",
    "list": {{ base_hyphens_toggle_set }},
    "scope": "global"
  */
  --product-title-hyphens: none;
  /* VoogStyle
    "pathI18n": ["product", "price"],
    "titleI18n": "size",
    "editor": "rangePicker",
    "min": 8,
    "max": 56,
    "unit": "px",
    "scope": "global"
  */
  --product-price-size: 14px;
  /* VoogStyle
    "pathI18n": ["product", "price"],
    "titleI18n": "font_size",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "700",
      "off": "400"
    },
    "icon": "bold",
    "scope": "global"
  */
  --product-price-font-weight: 400;
  /* VoogStyle
    "pathI18n": ["product", "price"],
    "titleI18n": "font_style",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "italic",
      "off": "normal"
    },
    "icon": "italic",
    "scope": "global"
  */
  --product-price-font-style: normal;
  /* VoogStyle
    "pathI18n": ["product", "price"],
    "titleI18n": "text_decoration",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "underline",
      "off": "none"
    },
    "icon": "underline",
    "scope": "global"
  */
  --product-price-text-decoration: none;
  /* VoogStyle
    "pathI18n": ["product", "price"],
    "titleI18n": "color",
    "type": "button",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global"
  */
  --product-price-color: #000000;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "size",
    "editor": "rangePicker",
    "min": 8,
    "max": 56,
    "unit": "px",
    "scope": "global"
  */
  --product-description-font-size: 14px;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "font_size",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "700",
      "off": "400"
    },
    "icon": "bold",
    "scope": "global"
  */
  --product-description-font-weight: 400;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "font_style",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "italic",
      "off": "normal"
    },
    "icon": "italic",
    "scope": "global"
  */
  --product-description-font-style: normal;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "text_decoration",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "underline",
      "off": "none"
    },
    "icon": "underline",
    "scope": "global"
  */
  --product-description-text-decoration: none;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "text_transform",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "uppercase",
      "off": "none"
    },
    "icon": "uppercase",
    "scope": "global"
  */
  --product-description-text-transform: none;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "color",
    "type": "button",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global"
  */
  --product-description-color: #000000;
  /* VoogStyle
    "pathI18n": ["product", "description"],
    "titleI18n": "hyphens",
    "editor": "listPicker",
    "list": {{ base_hyphens_toggle_set }},
    "scope": "global"
  */
  --product-description-hyphens: none;
}
