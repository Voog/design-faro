:root {
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "size",
    "editor": "rangePicker",
    "min": 8,
    "max": 56,
    "unit": "px",
    "scope": "global"
  */
  --form-label-font-size: 12px;
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "line_height",
    "editor": "rangePicker",
    "min": 0,
    "max": 5,
    "step": 0.1,
    "unit": "",
    "scope": "global"
  */
  --form-label-line-height: 1;
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "font_weight",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "700",
      "off": "400"
    },
    "icon": "bold",
    "scope": "global"
  */
  --form-label-font-weight: 700;
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "font_style",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "italic",
      "off": "normal"
    },
    "icon": "italic",
    "scope": "global"
  */
  --form-label-font-style: normal;
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "text_decoration",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "underline",
      "off": "none"
    },
    "icon": "underline",
    "scope": "global"
  */
  --form-label-text-decoration: none;
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "text_transform",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "uppercase",
      "off": "none"
    },
    "icon": "uppercase",
    "scope": "global"
  */
  --form-label-text-transform: none;
  /* VoogStyle
    "pathI18n": ["form", "label"],
    "titleI18n": "color",
    "type": "button",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global"
  */
  --form-label-color: #000000;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "field_size",
    "editor": "rangePicker",
    "min": 8,
    "max": 56,
    "unit": "px",
    "scope": "global"
  */
  --form-field-font-size: 12px;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "line_height",
    "editor": "rangePicker",
    "min": 1,
    "max": 5,
    "step": 0.1,
    "unit": "",
    "scope": "global"
  */
  --form-field-line-height: 1;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "font_weight",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "700",
      "off": "400"
    },
    "icon": "bold",
    "scope": "global"
  */
  --form-field-font-weight: 400;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "font_style",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "italic",
      "off": "normal"
    },
    "icon": "italic",
    "scope": "global"
  */
  --form-field-font-style: normal;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "text_decoration",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "underline",
      "off": "none"
    },
    "icon": "underline",
    "scope": "global"
  */
  --form-field-text-decoration: none;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "text_transform",
    "type": "button",
    "editor": "toggleIcon",
    "states": {
      "on": "uppercase",
      "off": "none"
    },
    "icon": "uppercase",
    "scope": "global"
  */
  --form-field-text-transform: none;
  /* VoogStyle
    "pathI18n": ["form", "form"],
    "titleI18n": "color",
    "type": "button",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global"
  */
  --form-field-color: #000000;
}
